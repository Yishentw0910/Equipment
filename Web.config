		<add key="DATAID" value="05" />
		<!-- DATAID(交易形態) -->
		<add key="DATAID" value="06" />
		<!-- DATAID(動用情形) -->
		<add key="DATAID" value="07" />
		<!-- DATAID(循環) -->
		<add key="DATAID" value="08" />
		<!-- DATAID(案件來源) -->
		<add key="DATAID" value="09" />
		<!-- DATAID(付款方式) -->
		<add key="DATAID" value="10" />
		<!-- DATAID(付款時間) -->
		<add key="DATAID" value="11" />
		<!-- DATAID(預計期滿處理方式) -->
		<add key="DATAID" value="12" />
		<!-- DATAID(回件資料) -->
		<add key="DATAID" value="13" />
		<!-- DATAID(標的物種類) -->
		<add key="DATAID" value="14" />
		<!-- DATAID(設備狀態) -->
		<add key="DATAID" value="15" />
		<!-- 上傳檔案共用路徑 -->
		<add key="UploadPath" value="D:\wwwroot\LE\Upload\" />
		<add key="UploadURL" value="./Upload/" />
		<add key="Upload_CNTRNO_URL" value="./MLUPLOAD/CNTRNO/" />
		<!-- 客服上傳檔案路徑 -->
		<add key="ML6001_Files" value="http://WBYHFC01/LE/CNTRNO_UPLOAD/" />
		<!-- 報表專用 WBYHFC01 -->
		<add key="SMART_QRY_POST_URL" value="http://apyhlc41/Smart-Query/WebLogin" />
		<add key="SMART_QRY_URL" value="http://apyhlc41/SEVI" />
		<add key="SMART_QRY_PATH" value="LE" />
		<add key="SMART_QRY_USER_NAME" value="lc" />
		<add key="SMART_QRY_PASS" value="1234" />
		<add key="SMART_QRY_COMPANY" value="HFC" />
		<!-- EMAIL 專用 -->
		<add key="MailServer" value="mail.hotaimotor.com.tw" />
		<add key="MailAccount" value="" />
		<add key="MailPassword" value="" />
		<add key="AttachmentPath" value="D:\wwwroot\LE\Upload\" />
		<!--FOR 正式機用 PP登入專用 -->
		<!--add key="LoginPP" value= "http://wbyhfc06/PP"/-->
		<!--FOR 測試機用 PP登入專用-->
		<add key="LoginPP" value="http://apyhlc01/PP" />
